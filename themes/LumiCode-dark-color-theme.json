{
    "name": "LumiCode Dark",
    "colors": {
        "editor.background": "#1E1E1E",
        "editor.foreground": "#D4D4D4",
        "list.activeSelectionIconForeground": "#FFFFFF",
        "editorLineNumber.foreground": "#858585",
        "editorCursor.foreground": "#D4D4D4",
        "editor.selectionBackground": "#264F78",
        "editor.lineHighlightBackground": "#333333"
    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#6A9955"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage"
            ],
            "settings": {
                "foreground": "#569CD6"
            }
        },
        {
            "name": "Types",
            "scope": [
                "storage.type",
                "support.type"
            ],
            "settings": {
                "foreground": "#4EC9B0"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#CE9178"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#DCDCAA"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "support.variable"
            ],
            "settings": {
                "foreground": "#9CDCFE"
            }
        },
        {
            "name": "Numbers, Constants",
            "scope": [
                "constant.numeric",
                "constant.character",
                "constant.language",
                "support.constant",
                "variable.language"
            ],
            "settings": {
                "foreground": "#B5CEA8"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.type",
                "entity.other.inherited-class",
                "support.class"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#4EC9B0"
            }
        },
        {
            "name": "Punctuation",
            "scope": "punctuation",
            "settings": {
                "foreground": "#D4D4D4"
            }
        }
    ]
}
